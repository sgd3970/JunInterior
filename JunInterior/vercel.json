{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1.js" }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "준장식프라자.com"
        }
      ],
      "destination": "https://www.준장식프라자.com/",
      "permanent": true,
      "statusCode": 301
    }
  ]
}
